public class NilaiRataRata 
{
    // ini adalah main method
    public static void main(String[] args) 
    {
        int number_1 = 10;
        int number_2 = 20;
        int number_3 = 45;
        
        // output yang akan tampil dilayar
        System.out.println("number 1 = "+number_1);
        System.out.println("number 2 = "+number_2);
        System.out.println("number 3 = "+number_3);
        System.out.println("Average is = "+(number_1+number_2+number_3)/3);
        
    }
}
